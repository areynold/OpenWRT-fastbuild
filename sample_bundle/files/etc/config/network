config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option proto 'commotion'
	option class 'client'
	option type 'bridge'

config interface 'wired'
	option ifname 'eth0'
	option proto 'commotion'
	option class 'wired'
